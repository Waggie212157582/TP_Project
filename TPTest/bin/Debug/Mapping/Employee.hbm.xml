<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="TP"
                   namespace="TP.Domain">
  <class name="Employee">
    <id name="id">
      <generator class="native" />
    </id>
    <property name="empId" />
    <property name="empNo" />
    <property name="empName" />
    <property name="empSurname" />
    <property name="empGender" />
    <property name="empAddress" />
    <property name="jobTitle" />
    <property name="empType" />
  </class>
</hibernate-mapping>