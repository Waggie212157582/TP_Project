<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="TP"
                   namespace="TP.Domain">
  <class name="PaymentData">
    <id name="id">
      <generator class="native" />
    </id>
    <property name="paymentDate" />
    <property name="paymentTime" />
    <property name="numberOfProcedures" />
    <property name="tax" />
    <property name="total" />
    <property name="changes" />
  </class>
</hibernate-mapping>